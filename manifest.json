{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Get Inspired",
    "short_name": "GI",
    "version": "1.0.0",
    "manifest_version": 2,
    "description" : "Inspire yourself when you open a new tab. 
    It is light weight extension so it doesn't affect your browsing speed",
    "offline_enabled": true,
    "chrome_url_overrides" : {
        "newtab": "app.html"
    },
    
 
    "icons": { "16": "images/icon16.png",
           "48": "images/icon48.png",
          "128": "images/icon128.png" }
}